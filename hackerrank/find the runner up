if __name__ == '__main__':
    n = int(input())
    arr = map(int, input().split())
    
i=0,j=1,max=0;
for x in arr:
    if(arr[i]>arr[j]):
        if(arr[i]>max):
            max = arr[i];
            i=i+1;
            j=j+1;
    else:
        i=i+1

